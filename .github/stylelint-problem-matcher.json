{
  "problemMatcher": [
    {
      "owner": "stylelint",
      "pattern": [
        {
          "regexp": "^([^\\s].*)$",
          "file": "1"
        },
        {
          "regexp": "^\\s+(\\d+):(\\d+)\\s+(✖|×)\\s+(.*)\\s{3,}(.*)$",
          "line": 1,
          "column": 2,
          "message": 4,
          "code": 5,
          "loop": true
        }
      ]
    }
  ]
}
